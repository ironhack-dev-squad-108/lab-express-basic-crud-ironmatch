<h1>Ranking</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Player</th>
      <th>Score</th>
      <th>Number of Matches</th>
    </tr>
  </thead>
  <tbody>
    {{#each players}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.score}}</td>
      <td>{{this.nbOfMatches}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
